<script>
if (typeof window !== 'undefined') {
	const cssUrl = '/_next/static/css/toolslogo-maker.1738856026058.css';
	const myWorker = new Worker('/js/fetchCssWorker.js');
	myWorker.postMessage({method:'fetchCSS',url:cssUrl});
	let css = null;
	window.addEventListener('DOMContentLoaded', () => {
		if (css !== null) {
			const style=document.createElement('style');style.textContent=css;document.head.appendChild(style);
		} else{
			const link=document.createElement('link');link.rel='stylesheet';link.href=cssUrl;document.head.appendChild(link);
		}
	});
	myWorker.addEventListener('message',(event) => { css=event.data.css ? event.data.css : null;});
}
