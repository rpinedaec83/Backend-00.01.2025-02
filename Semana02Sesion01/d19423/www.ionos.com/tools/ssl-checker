if (typeof window !== "undefined") {
	const tld = document.location.hostname.split('.').pop();
	if (tld !== 'localhost' && tld !== 'lan') {
		const abTestingRequest = new Request('/abtesting-' + tld + '.json');

		window.fetch(abTestingRequest)
			.then((response) => response.json())
			.then((data) => {
				let page = document.location.pathname;
				/* root page is special */
				if (page === '/') {
					page = 'Home';
				}
				if (data.pages && data.pages.includes && data.pages.includes(page)) {
					const targetScript = document.createElement('script');
					targetScript.src = '/js/at_visitor.min.js';
					targetScript.type = 'text/javascript';
					targetScript.onload = function() {
						/* once Target is loaded give it 3 seconds to apply the tests */
						window.setTimeout(function() {
							document.body.style.opacity = 1;
						}, 1000);
					};
					if (document.getElementsByTagName('head').length > 0) {
						document.getElementsByTagName('head')[0].appendChild(targetScript);
					}
					if (document?.body?.dataset) {
						document.body.dataset.abtesting = 1;
					} else {
						document.addEventListener("DOMContentLoaded", () => {
							document.body.dataset.abtesting = 1;
						});
					}
				} else {
					/* page is not under a/b testing -> unhide the body -> try directly */
					if (document?.body?.style?.opacity && document?.body?.style?.opacity != 'undefined' && document?.body?.style?.opacity == 0) {
						document.body.style.opacity = 1;
					} else {
						/* if not ready - try to unhide the body as early as possible */
						let intervalId = window.setInterval(() => {
							if (document?.body?.style?.opacity && document?.body?.style?.opacity != 'undefined' && document?.body?.style?.opacity == 0) {
								document.body.style.opacity = 1;
								window.clearInterval(intervalId);
							}
						}, 100);
					}

					document.addEventListener("DOMContentLoaded", () => {
						if (typeof intervalId !== "undefined") {
							window.clearInterval(intervalId);
						}
						document.body.style.opacity = 1;
						document.body.dataset.abtesting = 0;
					});
				}
			}, () => {
				/* Promise rejected -> unhide the body - no a/b testing */
				if (document?.body?.style?.opacity && document?.body?.style?.opacity != 'undefined' && document?.body?.style?.opacity == 0) {
					document.body.style.opacity = 1;
				} else {
					/* if not ready - try to unhide the body as early as possible */
					let intervalId = window.setInterval(() => {
						if (document?.body?.style?.opacity && document?.body?.style?.opacity != 'undefined' && document?.body?.style?.opacity == 0) {
							document.body.style.opacity = 1;
							window.clearInterval(intervalId);
						}
					}, 100);
				}

				document.addEventListener("DOMContentLoaded", () => {
					if (typeof intervalId !== "undefined") {
						window.clearInterval(intervalId);
					}
					document.body.style.opacity = 1;
					document.body.dataset.abtesting = 0;
				});
		});
	}
}</script><meta http-equiv="Content-Type" content="text/html; charset=UTF-8"><meta name="format-detection" content="telephone=no"><meta name="copyright" content="2001-2025 IONOS SE"><link rel="shortcut icon" href="/favicon.ico?v2" sizes="16x16 24x24 32x32 48x48 64x64 72x72 96x96" type="image/x-icon"><link rel="icon" type="image/png" href="/favicon-16x16.png" sizes="16x16"><link rel="icon" type="image/png" href="/favicon-24x24.png" sizes="24x24"><link rel="icon" type="image/png" href="/favicon-32x32.png" sizes="32x32"><link rel="icon" type="image/png" href="/favicon-48x48.png" sizes="48x48"><link rel="icon" type="image/png" href="/favicon-64x64.png" sizes="64x64"><link rel="icon" type="image/png" href="/favicon-72x72.png" sizes="72x72"><link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96"><link rel="icon" type="image/png" href="/favicon-256x256.png" sizes="256x256"><link rel="icon" href="/favicon.svg" sizes="any" type="image/svg+xml"><link rel="apple-touch-icon" type="image/png" href="/apple-touch-icon-180x180.png"><meta name="apple-mobile-web-app-title" content="IONOS"><meta name="msapplication-TileColor" content="#0b2a63"><meta name="theme-color" content="#001b41"><link rel="manifest" href="/site.webmanifest"><script defer nomodule src="/_next/static/chunks/polyfills-42372ed130431b0a.js"></script><script src="/js/sessions.js" defer data-nscript="beforeInteractive"></script><script defer src="/_next/static/chunks/2273-1738855411476.ddb13a897fa8ecf2.js"></script><script defer src="/_next/static/chunks/8874-1738855411476.0a3e426634e04662.js"></script><script defer src="/_next/static/chunks/7131-1738855411476.1d570c7843dda92c.js"></script><script defer src="/_next/static/chunks/2096-1738855411476.ce17872bb7840b84.js"></script><script defer src="/_next/static/chunks/1866-1738855411476.35ff76134127622d.js"></script><script defer src="/_next/static/chunks/8014-1738855411476.2f57e05622bcfa55.js"></script><script defer src="/_next/static/chunks/5713-1738